            <%= @inner_content %>
          </div>
        </div>
      </div>
    </main>

    <footer class="footer bg-gray-800 text-gray-600 mt-auto py-6">
      <div class="container mx-auto px-4 max-w-6xl">
        <span class="text-muted">Advance</span>
      </div>
    </footer>
  </body>
</html>
